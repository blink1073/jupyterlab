{
  "compilerOptions": {
    "declaration": true,
    "noImplicitAny": true,
    "noEmitOnError": true,
    "noUnusedLocals": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "ES5",
    "outDir": "./lib",
    "lib": ["ES5", "ES6", "DOM", "ES2015.Core", "ES2015.Collection", "ES2015.Promise"],
    "jsx": "react",
    "types": []
  },
  "include": ["src/*"]
}
