{
  "private": true,
  "scripts": {
    "install": "lerna bootstrap --hoist",
    "build:packages": "cd packages/all-packages && jlpm run build",
    "build:examples": "lerna run build --scope \"@jupyterlab/example-*\"",
    "build": "jlpm run build:packages && cd jupyterlab && jlpm run build",
    "build:main": "jlpm run build",
    "build:main:prod": "jlpm run build:packages && cd jupyterlab && jlpm run build:prod",
    "build:src": "lerna run build --scope \"@jupyterlab/!(test-|example-)*\"",
    "build:static": "cd jupyterlab && jlpm run build:static",
    "build:test": "lerna run build:test",
    "clean": "node scripts/clean-packages.js examples packages",
    "clean:examples": "node scripts/clean-packages.js examples",
    "clean:main": "cd jupyterlab && jlpm run clean",
    "clean:slate": "git clean -dfx && jlpm install --ignore-optional && jlpm run build",
    "clean:src": "node scripts/clean-packages.js packages",
    "clean:tests": "lerna run clean --scope \"@jupyterlab/test-*\"",
    "coverage": "lerna run coverage --stream",
    "docs": "lerna run docs",
    "get:dependency": "node scripts/get-dependency.js",
    "integrity": "node scripts/package-integrity.js",
    "publish": "jlpm run clean:slate && lerna publish --force-publish=* -m \"Publish\"",
    "test": "cd test && jlpm test",
    "test:services": "cd packages/services && jlpm test && jlpm run test:integration && cd examples/node && python main.py",
    "test:chrome": "lerna run test:chrome --stream",
    "test:firefox": "lerna run test:firefox --stream",
    "test:ie": "lerna run test:ie --concurrency 1 --stream",
    "update:dependency": "node scripts/update-dependency.js",
    "update:core": "cd jupyterlab && node update-core.js",
    "watch": "watch \"jlpm run build:packages\" ./packages/** --wait 10 --filter=scripts/watch-filter.js --ignoreDotFiles",
    "addsibling": "node scripts/add-sibling.js",
    "removesibling": "node scripts/remove-sibling.js"
  },
  "dependencies": {},
  "devDependencies": {
    "lerna": "^2.4.0"
  }
}
